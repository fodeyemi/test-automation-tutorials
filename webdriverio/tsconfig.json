{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": true,
    "target": "es5",
    "noEmit": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./"
      ],
      "test/*": [
        "./test/*"
      ]
    },
    "types": [
      "node",
      "webdriverio",
      "jest",
      "mocha"
    ]
  },
  "include": [
    "./test/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
